  {
    "@id": "https://w3id.org/tern/ontologies/ssn/DewPointHygrometer",
    "@type": [
      "http://www.w3.org/2000/01/rdf-schema#Class",
      "http://www.w3.org/2002/07/owl#Class"
    ],
    "http://www.w3.org/2000/01/rdf-schema#comment": [
      {
        "@value": "Dew-point hygrometers typically consist of a polished metal mirror that is cooled at a constant pressure and constant vapour content until moisture just starts to condense on it. The temperature of the metal at which condensation begins is the dew point."
      }
    ],
    "http://www.w3.org/2000/01/rdf-schema#label": [
      {
        "@value": "Dew point hygrometer"
      }
    ],
    "http://www.w3.org/2000/01/rdf-schema#seeAlso": [
      {
        "@id": "https://www.britannica.com/science/hygrometer#ref111343"
      }
    ],
