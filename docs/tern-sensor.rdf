    <rdfs:label>Sonic anemometer</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/tern/ontologies/sensor/Pyrradiometer">
    <rdfs:label>Pyrradiometer</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:seeAlso rdf:resource="https://www.energy.eu/dictionary/data/1446.html"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/tern/ontologies/sensor/Radiometer"/>
    <rdfs:comment>An instrument for measuring the total radiation, solar and terrestrial, falling from the solid angle 2 pi on a plane surface.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/tern/ontologies/sensor/EcologicalPlot">
