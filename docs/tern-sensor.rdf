  <rdf:Description rdf:about="https://w3id.org/tern/ontologies/sensor/ResistiveHygrometer">
    <rdfs:seeAlso rdf:resource="https://en.wikipedia.org/wiki/Hygrometer#Resistive"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/tern/ontologies/sensor/Hygrometer"/>
