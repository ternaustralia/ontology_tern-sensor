    <rdfs:label>Resistive hygrometer</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/tern/ontologies/sensor/Anemometer">
    <rdfs:subClassOf rdf:resource="https://w3id.org/tern/ontologies/sensor/WindSensor"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
