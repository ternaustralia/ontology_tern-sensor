    <rdfs:label>Weighing precipitation gauge</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:comment>A weighing-type precipitation gauge consists of a storage bin, which is weighed to record the mass.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/tern/ontologies/sensor/RainGauge"/>
