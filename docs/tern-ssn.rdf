    <rdfs:label>Quantum sensor</rdfs:label>
    <rdfs:seeAlso rdf:resource="https://en.wikipedia.org/wiki/Quantum_sensor"/>
    <rdfs:comment>A quantum sensor is a quantum device that responds to a stimulus. Usually this refers to a sensor which has quantized energy levels, uses quantum coherence to measure physical quantity, or uses entanglement to improve measurements beyond what can be done with classical sensors.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://w3id.org/tern/ontologies/ssn/RadiationSensor"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/tern/ontologies/ssn/HotWireAnemometer">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
