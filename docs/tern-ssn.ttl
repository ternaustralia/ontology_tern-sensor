# imports: http://www.w3.org/2004/02/skos/core
# imports: http://www.w3.org/ns/sosa/
# imports: https://raw.githack.com/w3c/sdw/gh-pages/ssn-extensions/rdf/ssn-ext.ttl
# imports: https://w3id.org/tern/ontologies/loc/
# imports: https://w3id.org/tern/ontologies/org/
# prefix: tern-ssn

@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dwc: <http://rs.tdwg.org/dwc/terms/> .
@prefix loc: <https://w3id.org/tern/ontologies/loc/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix ssn-system: <https://www.w3.org/ns/ssn/systems/> .
@prefix tern-org: <https://w3id.org/tern/ontologies/org/> .
@prefix tern-ssn: <https://w3id.org/tern/ontologies/ssn/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dwc:Identification
  a owl:Class ;
  rdfs:comment "A taxonomic determination (e.g., the assignment to a taxon)." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "Identification" ;
  skos:example "A subspecies determination of an organism." ;
.
dwc:MaterialSample
  a owl:Class ;
  rdfs:comment "A physical result of a sampling (or subsampling) event. In biological collections, the material sample is typically collected, and either preserved or destructively processed." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "Material Sample" ;
  skos:example "A whole organism preserved in a collection. A part of an organism isolated for some purpose. A soil sample. A marine microbial sample." ;
.
dwc:Taxon
  a owl:Class ;
  rdfs:comment "A group of organisms (sensu http://purl.obolibrary.org/obo/OBI_0100026) considered by taxonomists to form a homogeneous unit." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "Taxon" ;
  skos:example "The genus Truncorotaloides as published by Brönnimann et al. in 1953 in the Journal of Paleontology Vol. 27(6) p. 817-820." ;
.
dwc:class
  a rdf:Property ;
  rdfs:comment "The full scientific name of the class in which the taxon is classified." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "class" ;
.
dwc:dateIdentified
  a rdf:Property ;
  rdfs:comment "The date on which the subject was identified as representing the Taxon." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "dateIdentified" ;
  skos:example "1809-02-12 (some time during 12 February 1809)." ;
  skos:example "1900/1909 (some time during the interval between the beginning of the year 1900 and the end of the year 1909)." ;
  skos:example "1906-06 (some time in June 1906). " ;
  skos:example "1963-03-08T14:07-0600 (8 Mar 1963 at 2:07pm in the time zone six hours earlier than UTC)." ;
  skos:example "1971 (some time in the year 1971)." ;
  skos:example "2007-03-01T13:00:00Z/2008-05-11T15:30:00Z (some time during the interval between 1 March 2007 1pm UTC and 11 May 2008 3:30pm UTC)." ;
  skos:example "2007-11-13/15 (some time in the interval between 13 November 2007 and 15 November 2007)." ;
  skos:example "2009-02-20T08:40Z (20 February 2009 8:40am UTC)." ;
  skos:example "2018-08-29T15:19 (3:19pm local time on 29 August 2018)." ;
  skos:note "Recommended best practice is to use a date that conforms to ISO 8601:2004(E)." ;
.
dwc:family
  a rdf:Property ;
  rdfs:comment "The full scientific name of the family in which the taxon is classified." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "family" ;
  skos:example "Felidae, Monocleaceae" ;
.
dwc:genus
  a rdf:Property ;
  rdfs:comment "The full scientific name of the genus in which the taxon is classified." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "genus" ;
  skos:example "Puma, Monoclea" ;
.
dwc:identificationID
  a rdf:Property ;
  rdfs:comment "An identifier for the Identification (the body of information associated with the assignment of a scientific name). May be a global unique identifier or an identifier specific to the data set." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "identification ID" ;
  skos:example "9992" ;
.
dwc:identificationRemarks
  a rdf:Property ;
  rdfs:comment "Comments or notes about the Identification." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "identification remarks" ;
  skos:example "Distinguished between Anthus correndera and Anthus hellmayri based on the comparative lengths of the uñas." ;
.
dwc:identifiedBy
  a rdf:Property ;
  rdfs:comment "A list (concatenated and separated) of names of people, groups, or organizations who assigned the Taxon to the subject." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "identified by" ;
  skos:example "James L. Patton, Theodore Pappenfuss | Robert Macey" ;
  skos:note "Recommended best practice is to separate the values in a list with space vertical bar space ( | )." ;
.
dwc:infraspecificEpithet
  a rdf:Property ;
  rdfs:comment "The name of the lowest or terminal infraspecific epithet of the scientificName, excluding any rank designation." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "infraspecific epithet" ;
  skos:example "	concolor, oxyadenia, sayi" ;
.
dwc:kingdom
  a rdf:Property ;
  rdfs:comment "The full scientific name of the kingdom in which the taxon is classified." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "kingdom" ;
  skos:example "Animalia, Archaea, Bacteria, Chromista, Fungi, Plantae, Protozoa, Viruses" ;
.
dwc:materialSampleID
  a rdf:Property ;
  rdfs:comment "An identifier for the MaterialSample (as opposed to a particular digital record of the material sample). In the absence of a persistent global unique identifier, construct one from a combination of identifiers in the record that will most closely make the materialSampleID globally unique." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "material sample ID" ;
  skos:example "06809dc5-f143-459a-be1a-6f03e63fc083" ;
.
dwc:nameAccordingTo
  a rdf:Property ;
  rdfs:comment "The reference to the source in which the specific taxon concept circumscription is defined or implied - traditionally signified by the Latin \"sensu\" or \"sec.\" (from secundum, meaning \"according to\"). For taxa that result from identifications, a reference to the keys, monographs, experts and other sources should be given." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "name according to" ;
  skos:example "Lilljeborg 1861, Upsala Univ. Arsskrift, Math. Naturvet., pp. 4, 5" ;
  skos:example "McCranie, J. R., D. B. Wake, and L. D. Wilson. 1996. The taxonomic status of Bolitoglossa schmidti, with comments on the biology of the Mesoamerican salamander Bolitoglossa dofleini (Caudata: Plethodontidae). Carib. J. Sci. 32:395-398." ;
  skos:example "Werner Greuter 2008" ;
.
dwc:order
  a rdf:Property ;
  rdfs:comment "The full scientific name of the order in which the taxon is classified." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "order" ;
  skos:example "Carnivora, Monocleales" ;
.
dwc:phylum
  a rdf:Property ;
  rdfs:comment "The full scientific name of the phylum or division in which the taxon is classified." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "phylum" ;
  skos:example "Chordata (phylum). Bryophyta (division)." ;
.
dwc:scientificName
  a rdf:Property ;
  rdfs:comment "The full scientific name, with authorship and date information if known. When forming part of an Identification, this should be the name in lowest level taxonomic rank that can be determined. This term should not contain identification qualifications, which should instead be supplied in the IdentificationQualifier term." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "scientific name" ;
.
dwc:scientificNameAuthorship
  a rdf:Property ;
  rdfs:comment "The authorship information for the scientificName formatted according to the conventions of the applicable nomenclaturalCode." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "scientific name authorship" ;
  skos:example "(Torr.) J.T. Howell, (Martinovský) Tzvelev, (Györfi, 1952)" ;
.
dwc:scientificNameID
  a rdf:Property ;
  rdfs:comment "An identifier for the nomenclatural (not taxonomic) details of a scientific name." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "scientific name ID" ;
  skos:example "urn:lsid:ipni.org:names:37829-1:1.3" ;
.
dwc:specificEpithet
  a rdf:Property ;
  rdfs:comment "The name of the first or species epithet of the scientificName." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "specific epithet" ;
  skos:example "concolor, gottschei" ;
.
dwc:taxonConceptID
  a rdf:Property ;
  rdfs:comment "An identifier for the taxonomic concept to which the record refers - not for the nomenclatural details of a taxon." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "taxon concept ID" ;
  skos:example "8fa58e08-08de-4ac1-b69c-1235340b7001" ;
.
dwc:taxonRemarks
  a rdf:Property ;
  rdfs:comment "Comments or notes about the taxon or name." ;
  rdfs:isDefinedBy dwc: ;
  rdfs:label "taxon remarks" ;
  skos:example "this name is a misspelling in common use" ;
.
schema:OwnershipInfo
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              schema:Organization
              schema:Person
            ) ;
        ] ;
      owl:onProperty schema:acquiredFrom ;
    ] ;
.
schema:manufacturer
  rdfs:range tern-org:Manufacturer ;
.
<http://www.w3.org/ns/prov#Activity>
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#endedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#startedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
      owl:someValuesFrom schema:Person ;
    ] ;
.
<http://www.w3.org/ns/prov#Entity>
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Person ;
      owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty schema:description ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ;
    ] ;
.
sosa:ObservableProperty
  a rdfs:Class ;
  a owl:Class ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "Observable Property"@en ;
.
sosa:Platform
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "Platform"@en ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Place ;
      owl:onProperty schema:location ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty schema:funder ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:collaboratorOrganization ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:dataManager ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:landOwner ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:principalInvestigator ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:technician ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom skos:Concept ;
      owl:onProperty tern-ssn:landOwnershipType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Deployment ;
      owl:onProperty ssn:inDeployment ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:System ;
      owl:onProperty sosa:hosts ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty schema:identifier ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty schema:name ;
    ] ;
  skos:definition "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms."@en ;
  skos:example "A post, buoy, vehicle, ship, aircraft, satellite, cell-phone, human or animal may act as platforms for (technical or biological) sensors or actuators."@en ;
.
sosa:Procedure
  a rdfs:Class ;
  a owl:Class ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "Procedure"@en ;
.
sosa:Sensor
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "Device, agent (including humans), or software (simulation) involved in, or implementing, a Procedure. Sensors respond to a stimulus, e.g., a change in the environment, or input data composed from the results of prior Observations, and generate a Result. Sensors can be hosted by Platforms."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "Sensor"@en ;
  rdfs:subClassOf ssn:System ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Organization ;
      owl:onProperty schema:manufacturer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom skos:Concept ;
      owl:onProperty schema:model ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:ObservableProperty ;
      owl:onProperty sosa:observes ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Observation ;
      owl:onProperty sosa:madeObservation ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Platform ;
      owl:onProperty sosa:isHostedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Deployment ;
      owl:onProperty ssn:hasDeployment ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Stimulus ;
      owl:onProperty ssn:detects ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom tern-ssn:Calibration ;
      owl:onProperty tern-ssn:hasCalibration ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom tern-ssn:SensorConfiguration ;
      owl:onProperty tern-ssn:hasConfiguration ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty schema:model ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-ssn:isDeployed ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty schema:manufacturer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ssn:implements ;
    ] ;
  skos:definition "Device, agent (including humans), or software (simulation) involved in, or implementing, a Procedure. Sensors respond to a stimulus, e.g., a change in the environment, or input data composed from the results of prior Observations, and generate a Result. Sensors can be hosted by Platforms."@en ;
  skos:example "Accelerometers, gyroscopes, barometers, magnetometers, and so forth are Sensors that are typically mounted on a modern smart phone (which acts as Platform). Other examples of sensors include the human eyes."@en ;
.
sosa:hosts
  a owl:ObjectProperty ;
  schema:domainIncludes sosa:Platform ;
  schema:rangeIncludes sosa:Actuator ;
  schema:rangeIncludes sosa:Platform ;
  schema:rangeIncludes sosa:Sampler ;
  schema:rangeIncludes sosa:Sensor ;
  rdfs:comment "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "hosts"@en ;
  owl:inverseOf sosa:isHostedBy ;
  owl:propertyChainAxiom (
      ssn:inDeployment
      ssn:deployedSystem
    ) ;
  skos:definition "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en ;
.
sosa:isHostedBy
  a owl:ObjectProperty ;
  schema:domainIncludes sosa:Actuator ;
  schema:domainIncludes sosa:Platform ;
  schema:domainIncludes sosa:Sampler ;
  schema:domainIncludes sosa:Sensor ;
  schema:rangeIncludes sosa:Platform ;
  rdfs:comment "Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "is hosted by"@en ;
  owl:inverseOf sosa:hosts ;
  skos:definition "Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by."@en ;
.
sosa:observes
  a owl:ObjectProperty ;
  schema:domainIncludes sosa:Sensor ;
  schema:rangeIncludes sosa:ObservableProperty ;
  rdfs:comment "Relation between a Sensor and an ObservableProperty that it is capable of sensing."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "observes"@en ;
  owl:inverseOf sosa:isObservedBy ;
  skos:definition "Relation between a Sensor and an ObservableProperty that it is capable of sensing."@en ;
.
ssn:Deployment
  a owl:Class ;
  rdfs:comment "Describes the Deployment of one or more Systems for a particular purpose. Deployment may be done on a Platform."@en ;
  rdfs:isDefinedBy ssn: ;
  rdfs:label "Deployment"@en ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Place ;
      owl:onProperty schema:location ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:deploymentDepth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:deploymentHeight ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:deploymentOrientation ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty schema:description ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom skos:Concept ;
      owl:onProperty tern-ssn:deploymentType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Platform ;
      owl:onProperty ssn:deployedOnPlatform ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Property ;
      owl:onProperty ssn:forProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:System ;
      owl:onProperty ssn:deployedSystem ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
      owl:someValuesFrom schema:Person ;
    ] ;
  skos:definition "Describes the Deployment of one or more Systems for a particular purpose. Deployment may be done on a Platform."@en ;
  skos:example "For example, a temperature Sensor deployed on a wall, or a whole network of Sensors deployed for an Observation campaign."@en ;
.
ssn:System
  a owl:Class ;
  rdfs:comment "System is a unit of abstraction for pieces of infrastructure that implement Procedures. A System may have components, its subsystems, which are other systems."@en ;
  rdfs:isDefinedBy ssn: ;
  rdfs:label "System"@en ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Platform ;
      owl:onProperty sosa:isHostedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Procedure ;
      owl:onProperty ssn:implements ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Deployment ;
      owl:onProperty ssn:hasDeployment ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:System ;
      owl:onProperty ssn:hasSubSystem ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:System ;
      owl:onProperty [
          owl:inverseOf ssn:hasSubSystem ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn-system:OperatingRange ;
      owl:onProperty ssn-system:hasOperatingRange ;
