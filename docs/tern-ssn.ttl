# baseURI: https://w3id.org/tern/ontologies/ssn/
# imports: http://www.w3.org/2004/02/skos/core
# imports: https://w3id.org/tern/ontologies/org/
# prefix: tern-ssn

@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix ssn-system: <https://www.w3.org/ns/ssn/systems/> .
@prefix tern-org: <https://w3id.org/tern/ontologies/org/> .
@prefix tern-ssn: <https://w3id.org/tern/ontologies/ssn/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

schema:OwnershipInfo
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom [
          a owl:Class ;
          owl:unionOf (
              schema:Organization
              schema:Person
            ) ;
        ] ;
      owl:onProperty schema:acquiredFrom ;
    ] ;
.
schema:manufacturer
  rdfs:range tern-org:Manufacturer ;
.
<http://www.w3.org/ns/prov#Activity>
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#endedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#startedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
      owl:someValuesFrom schema:Person ;
    ] ;
.
<http://www.w3.org/ns/prov#Entity>
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Person ;
      owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty schema:description ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ;
    ] ;
.
sosa:Platform
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "Platform"@en ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Place ;
      owl:onProperty schema:location ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty schema:funder ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:collaboratorOrganization ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:dataManager ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:landOwner ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:principalInvestigator ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Role ;
      owl:onProperty tern-org:technician ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom skos:Concept ;
      owl:onProperty tern-ssn:landOwnershipType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Deployment ;
      owl:onProperty ssn:inDeployment ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:System ;
      owl:onProperty sosa:hosts ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty schema:identifier ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty schema:name ;
    ] ;
  skos:definition "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms."@en ;
  skos:example "A post, buoy, vehicle, ship, aircraft, satellite, cell-phone, human or animal may act as platforms for (technical or biological) sensors or actuators."@en ;
.
sosa:Sensor
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "Device, agent (including humans), or software (simulation) involved in, or implementing, a Procedure. Sensors respond to a stimulus, e.g., a change in the environment, or input data composed from the results of prior Observations, and generate a Result. Sensors can be hosted by Platforms."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "Sensor"@en ;
  rdfs:subClassOf ssn:System ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Organization ;
      owl:onProperty schema:manufacturer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom skos:Concept ;
      owl:onProperty schema:model ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:ObservableProperty ;
      owl:onProperty sosa:observes ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Observation ;
      owl:onProperty sosa:madeObservation ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Platform ;
      owl:onProperty sosa:isHostedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Deployment ;
      owl:onProperty ssn:hasDeployment ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Stimulus ;
      owl:onProperty ssn:detects ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom tern-ssn:Calibration ;
      owl:onProperty tern-ssn:hasCalibration ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom tern-ssn:SensorConfiguration ;
      owl:onProperty tern-ssn:hasConfiguration ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty schema:model ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-ssn:isDeployed ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty schema:manufacturer ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:minCardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty ssn:implements ;
    ] ;
  skos:definition "Device, agent (including humans), or software (simulation) involved in, or implementing, a Procedure. Sensors respond to a stimulus, e.g., a change in the environment, or input data composed from the results of prior Observations, and generate a Result. Sensors can be hosted by Platforms."@en ;
  skos:example "Accelerometers, gyroscopes, barometers, magnetometers, and so forth are Sensors that are typically mounted on a modern smart phone (which acts as Platform). Other examples of sensors include the human eyes."@en ;
.
sosa:hosts
  a owl:ObjectProperty ;
  schema:domainIncludes sosa:Platform ;
  schema:rangeIncludes sosa:Actuator ;
  schema:rangeIncludes sosa:Platform ;
  schema:rangeIncludes sosa:Sampler ;
  schema:rangeIncludes sosa:Sensor ;
  rdfs:comment "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "hosts"@en ;
  owl:inverseOf sosa:isHostedBy ;
  owl:propertyChainAxiom (
      ssn:inDeployment
      ssn:deployedSystem
    ) ;
  skos:definition "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en ;
.
sosa:isHostedBy
  a owl:ObjectProperty ;
  schema:domainIncludes sosa:Actuator ;
  schema:domainIncludes sosa:Platform ;
  schema:domainIncludes sosa:Sampler ;
  schema:domainIncludes sosa:Sensor ;
  schema:rangeIncludes sosa:Platform ;
  rdfs:comment "Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "is hosted by"@en ;
  owl:inverseOf sosa:hosts ;
  skos:definition "Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by."@en ;
.
sosa:observes
  a owl:ObjectProperty ;
  schema:domainIncludes sosa:Sensor ;
  schema:rangeIncludes sosa:ObservableProperty ;
  rdfs:comment "Relation between a Sensor and an ObservableProperty that it is capable of sensing."@en ;
  rdfs:isDefinedBy sosa: ;
  rdfs:label "observes"@en ;
  owl:inverseOf sosa:isObservedBy ;
  skos:definition "Relation between a Sensor and an ObservableProperty that it is capable of sensing."@en ;
.
ssn:Deployment
  a owl:Class ;
  rdfs:comment "Describes the Deployment of one or more Systems for a particular purpose. Deployment may be done on a Platform."@en ;
  rdfs:isDefinedBy ssn: ;
  rdfs:label "Deployment"@en ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:Place ;
      owl:onProperty schema:location ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:deploymentDepth ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:deploymentHeight ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:deploymentOrientation ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:dateTime ;
      owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty schema:description ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom skos:Concept ;
      owl:onProperty tern-ssn:deploymentType ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
      owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Platform ;
      owl:onProperty ssn:deployedOnPlatform ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Property ;
      owl:onProperty ssn:forProperty ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:System ;
      owl:onProperty ssn:deployedSystem ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
      owl:someValuesFrom schema:Person ;
    ] ;
  skos:definition "Describes the Deployment of one or more Systems for a particular purpose. Deployment may be done on a Platform."@en ;
  skos:example "For example, a temperature Sensor deployed on a wall, or a whole network of Sensors deployed for an Observation campaign."@en ;
.
ssn:System
  a owl:Class ;
  rdfs:comment "System is a unit of abstraction for pieces of infrastructure that implement Procedures. A System may have components, its subsystems, which are other systems."@en ;
  rdfs:isDefinedBy ssn: ;
  rdfs:label "System"@en ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Platform ;
      owl:onProperty sosa:isHostedBy ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom sosa:Procedure ;
      owl:onProperty ssn:implements ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:Deployment ;
      owl:onProperty ssn:hasDeployment ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:System ;
      owl:onProperty ssn:hasSubSystem ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn:System ;
      owl:onProperty [
          owl:inverseOf ssn:hasSubSystem ;
        ] ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn-system:OperatingRange ;
      owl:onProperty ssn-system:hasOperatingRange ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn-system:SurvivalRange ;
      owl:onProperty ssn-system:hasSurvivalRange ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom ssn-system:SystemCapability ;
      owl:onProperty ssn-system:hasSystemCapability ;
    ] ;
  skos:definition "System is a unit of abstraction for pieces of infrastructure that implement Procedures. A System may have components, its subsystems, which are other systems."@en ;
.
ssn:deployedOnPlatform
  a owl:ObjectProperty ;
  rdfs:comment "Relation between a Deployment and the Platform on which the Systems are deployed."@en ;
  rdfs:isDefinedBy ssn: ;
  rdfs:label "deployed on platform"@en ;
  owl:inverseOf ssn:inDeployment ;
  skos:definition "Relation between a Deployment and the Platform on which the Systems are deployed."@en ;
.
ssn:deployedSystem
  a owl:ObjectProperty ;
  rdfs:comment "Relation between a Deployment and a deployed System."@en ;
  rdfs:isDefinedBy ssn: ;
  rdfs:label "deployed system"@en ;
  owl:inverseOf ssn:hasDeployment ;
  skos:definition "Relation between a Deployment and a deployed System."@en ;
.
ssn:hasDeployment
  a owl:ObjectProperty ;
  rdfs:comment "Relation between a System and a Deployment, recording that the System is deployed in that Deployment."@en ;
  rdfs:isDefinedBy ssn: ;
  rdfs:label "has deployment"@en ;
  owl:inverseOf ssn:deployedSystem ;
  skos:definition "Relation between a System and a Deployment, recording that the System is deployed in that Deployment."@en ;
.
ssn:inDeployment
  a owl:ObjectProperty ;
  rdfs:comment "Relation between a Platform and a Deployment, meaning that the deployedSystems of the Deployment are hosted on the Platform."@en ;
  rdfs:isDefinedBy ssn: ;
  rdfs:label "in deployment"@en ;
  owl:inverseOf ssn:deployedOnPlatform ;
  skos:definition "Relation between a Platform and a Deployment, meaning that the deployedSystems of the Deployment are hosted on the Platform."@en ;
  skos:example "For example, a relation between a buoy and a deployment of several Sensors."@en ;
.
tern-ssn:
  a owl:Ontology ;
  dct:created "2020-01-07"^^xsd:date ;
  dct:creator [
      a schema:Person ;
      schema:affiliation <https://w3id.org/tern/resources/a083902d-d821-41be-b663-1d7cb33eea66> ;
      schema:email "e.chuc@uq.edu.au" ;
      schema:identifier "https://orcid.org/0000-0002-6047-9864" ;
      schema:name "Edmond Chuc" ;
    ] ;
  dct:modified "2020-03-23"^^xsd:date ;
  dct:publisher <https://w3id.org/tern/resources/a083902d-d821-41be-b663-1d7cb33eea66> ;
  vann:preferredNamespacePrefix "tern-ssn" ;
  vann:preferredNamespaceUri "https://w3id.org/tern/ontologies/ssn/" ;
  rdfs:comment "A vocabulary of TERN sensors and platforms and an extension of SOSA, SSN, and SSN System." ;
  rdfs:label "TERN SSN ontology extension and vocabulary" ;
  rdfs:seeAlso <https://github.com/ternaustralia/ontology_tern-ssn> ;
  owl:imports <http://www.w3.org/2004/02/skos/core> ;
  owl:imports tern-org: ;
  owl:versionIRI <https://w3id.org/tern/ontologies/ssn/0.0.2> ;
  owl:versionInfo "0.0.2" ;
.
<https://w3id.org/tern/ontologies/ssn/2DSonicAnemometer>
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "Two-dimensional (wind speed and wind direction) sonic anemometers are used in applications such as weather stations, ship navigation, wind turbines, aviation and weather buoys." ;
  rdfs:label "2D sonic anemometer" ;
  rdfs:subClassOf tern-ssn:SonicAnemometer ;
.
<https://w3id.org/tern/ontologies/ssn/3DSonicAnemometer>
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "Three-dimensional sonic anemometers are widely used to measure gas emissions and ecosystem fluxes using the eddy covariance method when used with fast-response infrared gas analyzers or laser-based analyzers." ;
  rdfs:label "3D sonic anemometer" ;
  rdfs:subClassOf tern-ssn:SonicAnemometer ;
.
tern-ssn:Actinometer
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "Actinometers are instruments used to measure the heating power of radiation." ;
  rdfs:label "Actinometer" ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Actinometer> ;
  rdfs:subClassOf tern-ssn:Radiometer ;
.
tern-ssn:AffiliateSuperSite
  a rdfs:Class ;
  a owl:Class ;
  rdfs:label "Affiliate super site" ;
  rdfs:subClassOf tern-ssn:Site ;
.
tern-ssn:Aircraft
  a rdfs:Class ;
  a owl:Class ;
  rdfs:label "Aircraft" ;
  rdfs:subClassOf tern-ssn:MobilePlatform ;
.
tern-ssn:Anemometer
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "An anemometer is a device used for measuring wind speed and direction." ;
  rdfs:label "Anemometer" ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Anemometer> ;
  rdfs:subClassOf tern-ssn:WindSensor ;
.
tern-ssn:AneroidBarometer
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "An aneroid barometer is an instrument used for measuring pressure as a method that does not involve liquid." ;
  rdfs:label "Aneroid barometer" ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Barometer#Aneroid_barometers> ;
  rdfs:subClassOf tern-ssn:Barometer ;
.
tern-ssn:Barometer
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "A barometer is a scientific instrument that is used to measure air pressure in a certain environment." ;
  rdfs:label "Barometer" ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Barometer> ;
  rdfs:subClassOf sosa:Sensor ;
.
tern-ssn:Calibration
  a owl:Class ;
  rdfs:label "Calibration" ;
  rdfs:subClassOf <http://www.w3.org/ns/prov#Entity> ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:actualResult ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:calibrationOffset ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:calibrationSlope ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom schema:QuantitativeValue ;
      owl:onProperty tern-ssn:referenceResult ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:allValuesFrom xsd:string ;
      owl:onProperty schema:description ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-ssn:actualResult ;
    ] ;
  rdfs:subClassOf [
      a owl:Restriction ;
      owl:cardinality "1"^^xsd:nonNegativeInteger ;
      owl:onProperty tern-ssn:referenceResult ;
    ] ;
.
tern-ssn:CapactitiveSensor
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "A capacitive sensor is a proximity sensor that detects nearby objects by their effect on the electrical field created by the sensor." ;
  rdfs:label "Capactitive sensor" ;
  rdfs:seeAlso <http://www.cs.cmu.edu/~ram/capsense/intro.html> ;
  rdfs:subClassOf sosa:Sensor ;
.
tern-ssn:Ceilometer
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "A ceilometer is a device that uses a laser or other light source to determine the height of a cloud ceiling or cloud base. Ceilometers can also be used to measure the aerosol concentration within the atmosphere. When based on laser, it is a type of atmospheric lidar." ;
  rdfs:label "Ceilometer" ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Ceilometer> ;
  rdfs:subClassOf sosa:Sensor ;
.
tern-ssn:CupAnemometer
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "A cup anemometer consists of four hemispherical cups mounted on horizontal arms, mounted on a vertical shaft." ;
  rdfs:label "Cup anemometer" ;
  rdfs:seeAlso <https://en.wikipedia.org/wiki/Anemometer#Cup_anemometers> ;
  rdfs:subClassOf tern-ssn:Anemometer ;
.
tern-ssn:Dendrometer
  a rdfs:Class ;
  a owl:Class ;
  rdfs:comment "Dendrometers are precision sensors that can measure the growth of plants. More specifically, dendrometers are used by researchers, growers and foresters to measure the growth of a stem, trunk, branch, roots or fruits over time." ;
  rdfs:label "Dendrometer" ;
  rdfs:seeAlso <https://www.edaphic.com.au/dendrometers-multi-purpose-sensors/> ;
