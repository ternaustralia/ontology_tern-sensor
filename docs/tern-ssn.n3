@prefix dct: <http://purl.org/dc/terms/> .
@prefix dwc: <http://rs.tdwg.org/dwc/terms/> .
@prefix loc: <https://w3id.org/tern/ontologies/loc/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <http://schema.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix ssn: <http://www.w3.org/ns/ssn/> .
@prefix ssn-system: <https://www.w3.org/ns/ssn/systems/> .
@prefix tern-org: <https://w3id.org/tern/ontologies/org/> .
@prefix tern-ssn: <https://w3id.org/tern/ontologies/ssn/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

sosa:ObservableProperty a rdfs:Class,
        owl:Class ;
    rdfs:label "Observable Property"@en ;
    rdfs:isDefinedBy sosa: .

sosa:Platform a rdfs:Class,
        owl:Class ;
    rdfs:label "Platform"@en ;
    rdfs:comment "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms."@en ;
    rdfs:isDefinedBy sosa: ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom schema:Place ;
            owl:onProperty schema:location ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:Role ;
            owl:onProperty schema:funder ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:Role ;
            owl:onProperty tern-org:collaboratorOrganization ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:Role ;
            owl:onProperty tern-org:dataManager ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:Role ;
            owl:onProperty tern-org:landOwner ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:Role ;
            owl:onProperty tern-org:principalInvestigator ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:Role ;
            owl:onProperty tern-org:technician ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ],
        [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty tern-ssn:landOwnershipType ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:Deployment ;
            owl:onProperty ssn:inDeployment ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:System ;
            owl:onProperty sosa:hosts ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty schema:identifier ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty schema:name ] ;
    skos:definition "A Platform is an entity that hosts other entities, particularly Sensors, Actuators, Samplers, and other Platforms."@en ;
    skos:example "A post, buoy, vehicle, ship, aircraft, satellite, cell-phone, human or animal may act as platforms for (technical or biological) sensors or actuators."@en .

sosa:Procedure a rdfs:Class,
        owl:Class ;
    rdfs:label "Procedure"@en ;
    rdfs:isDefinedBy sosa: .

sosa:Sensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Sensor"@en ;
    rdfs:comment "Device, agent (including humans), or software (simulation) involved in, or implementing, a Procedure. Sensors respond to a stimulus, e.g., a change in the environment, or input data composed from the results of prior Observations, and generate a Result. Sensors can be hosted by Platforms."@en ;
    rdfs:isDefinedBy sosa: ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom schema:Organization ;
            owl:onProperty schema:manufacturer ],
        [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty schema:model ],
        [ a owl:Restriction ;
            owl:allValuesFrom sosa:ObservableProperty ;
            owl:onProperty sosa:observes ],
        [ a owl:Restriction ;
            owl:allValuesFrom sosa:Observation ;
            owl:onProperty sosa:madeObservation ],
        [ a owl:Restriction ;
            owl:allValuesFrom sosa:Platform ;
            owl:onProperty sosa:isHostedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:Deployment ;
            owl:onProperty ssn:hasDeployment ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:Stimulus ;
            owl:onProperty ssn:detects ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern-ssn:Calibration ;
            owl:onProperty tern-ssn:hasCalibration ],
        [ a owl:Restriction ;
            owl:allValuesFrom tern-ssn:SensorConfiguration ;
            owl:onProperty tern-ssn:hasConfiguration ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty schema:model ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty tern-ssn:isDeployed ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty schema:manufacturer ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty ssn:implements ],
        ssn:System ;
    skos:definition "Device, agent (including humans), or software (simulation) involved in, or implementing, a Procedure. Sensors respond to a stimulus, e.g., a change in the environment, or input data composed from the results of prior Observations, and generate a Result. Sensors can be hosted by Platforms."@en ;
    skos:example "Accelerometers, gyroscopes, barometers, magnetometers, and so forth are Sensors that are typically mounted on a modern smart phone (which acts as Platform). Other examples of sensors include the human eyes."@en .

<https://w3id.org/tern/ontologies/ssn/2DSonicAnemometer> a rdfs:Class,
        owl:Class ;
    rdfs:label "2D sonic anemometer" ;
    rdfs:comment "Two-dimensional (wind speed and wind direction) sonic anemometers are used in applications such as weather stations, ship navigation, wind turbines, aviation and weather buoys." ;
    rdfs:subClassOf tern-ssn:SonicAnemometer .

<https://w3id.org/tern/ontologies/ssn/3DSonicAnemometer> a rdfs:Class,
        owl:Class ;
    rdfs:label "3D sonic anemometer" ;
    rdfs:comment "Three-dimensional sonic anemometers are widely used to measure gas emissions and ecosystem fluxes using the eddy covariance method when used with fast-response infrared gas analyzers or laser-based analyzers." ;
    rdfs:subClassOf tern-ssn:SonicAnemometer .

tern-ssn:Actinometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Actinometer" ;
    rdfs:comment "Actinometers are instruments used to measure the heating power of radiation." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Actinometer> ;
    rdfs:subClassOf tern-ssn:Radiometer .

tern-ssn:AffiliateSuperSite a rdfs:Class,
        owl:Class ;
    rdfs:label "Affiliate SuperSite" ;
    rdfs:comment "A site that is part of the SuperSite network but is not directly funded by TERN." ;
    rdfs:subClassOf tern-ssn:SuperSite .

tern-ssn:Aircraft a rdfs:Class,
        owl:Class ;
    rdfs:label "Aircraft" ;
    rdfs:subClassOf sosa:Platform .

tern-ssn:Anemometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Anemometer" ;
    rdfs:comment "An anemometer is a device used for measuring wind speed and direction." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Anemometer> ;
    rdfs:subClassOf tern-ssn:WindSensor .

tern-ssn:AneroidBarometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Aneroid barometer" ;
    rdfs:comment "An aneroid barometer is an instrument used for measuring pressure as a method that does not involve liquid." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Barometer#Aneroid_barometers> ;
    rdfs:subClassOf tern-ssn:Barometer .

tern-ssn:AutomaticWeatherStation a rdfs:Class,
        owl:Class ;
    rdfs:label "Automatic weather station" ;
    rdfs:subClassOf tern-ssn:InSituLandBasedPlatform .

tern-ssn:Barometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Barometer" ;
    rdfs:comment "A barometer is a scientific instrument that is used to measure air pressure in a certain environment." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Barometer> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:CORVEGSite a rdfs:Class,
        owl:Class ;
    rdfs:label "CORVEG Site" ;
    rdfs:subClassOf tern-ssn:EcologySite .

tern-ssn:CapactitiveSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Capactitive sensor" ;
    rdfs:comment "A capacitive sensor is a proximity sensor that detects nearby objects by their effect on the electrical field created by the sensor." ;
    rdfs:seeAlso <http://www.cs.cmu.edu/~ram/capsense/intro.html> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:CategoricalVariable a rdfs:Class,
        owl:Class ;
    rdfs:label "Categorical Variable" ;
    rdfs:subClassOf skos:Concept,
        ssn:Property .

tern-ssn:Ceilometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Ceilometer" ;
    rdfs:comment "A ceilometer is a device that uses a laser or other light source to determine the height of a cloud ceiling or cloud base. Ceilometers can also be used to measure the aerosol concentration within the atmosphere. When based on laser, it is a type of atmospheric lidar." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Ceilometer> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:CollectedSample a rdfs:Class,
        owl:Class ;
    rdfs:label "Collected sample" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dwc:materialSampleID ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:allValuesFrom loc:Point ;
            owl:onProperty tern-ssn:sampleStorageLocation ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dwc:materialSampleID ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty rdfs:label ],
        dwc:MaterialSample,
        sosa:Sample .

tern-ssn:CosmOzSite a rdfs:Class,
        owl:Class ;
    rdfs:label "CosmOz site" ;
    rdfs:subClassOf tern-ssn:EcologySite .

tern-ssn:CupAnemometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Cup anemometer" ;
    rdfs:comment "A cup anemometer consists of four hemispherical cups mounted on horizontal arms, mounted on a vertical shaft." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Anemometer#Cup_anemometers> ;
    rdfs:subClassOf tern-ssn:Anemometer .

tern-ssn:Dendrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Dendrometer" ;
    rdfs:comment "Dendrometers are precision sensors that can measure the growth of plants. More specifically, dendrometers are used by researchers, growers and foresters to measure the growth of a stem, trunk, branch, roots or fruits over time." ;
    rdfs:seeAlso <https://www.edaphic.com.au/dendrometers-multi-purpose-sensors/>,
        <https://www.edaphic.com.au/products/dendrometers/> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:DewPointHygrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Dew point hygrometer" ;
    rdfs:comment "Dew-point hygrometers typically consist of a polished metal mirror that is cooled at a constant pressure and constant vapour content until moisture just starts to condense on it. The temperature of the metal at which condensation begins is the dew point." ;
    rdfs:seeAlso <https://www.britannica.com/science/hygrometer#ref111343> ;
    rdfs:subClassOf tern-ssn:Hygrometer .

tern-ssn:EarthObservationSatellite a rdfs:Class,
        owl:Class ;
    rdfs:label "Earth observation satellite" ;
    rdfs:subClassOf sosa:Platform .

tern-ssn:EcologySite a rdfs:Class,
        owl:Class ;
    rdfs:label "Ecology site" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tern-ssn:fluxnetID ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tern-ssn:locationDescription ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty tern-ssn:ozfluxID ],
        tern-ssn:InSituLandBasedPlatform .

tern-ssn:EcosystemProcessesSite a rdfs:Class,
        owl:Class ;
    rdfs:label "Ecosystem processes site" ;
    rdfs:subClassOf tern-ssn:EcologySite .

tern-ssn:EddyCovarianceFluxTower a rdfs:Class,
        owl:Class ;
    rdfs:label "Eddy covariance flux tower" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty tern-ssn:climateCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty tern-ssn:igbpCode ],
        [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty tern-ssn:powerSupplyType ],
        [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty tern-ssn:terrainType ],
        [ a owl:Restriction ;
            owl:onProperty tern-ssn:fluxnetID ;
            owl:someValuesFrom xsd:string ],
        tern-ssn:InSituLandBasedPlatform .

tern-ssn:ElectromagneticRadiationHygrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Electromagnetic radiation absorption hygrometer" ;
    rdfs:subClassOf tern-ssn:Hygrometer .

tern-ssn:ElectronicBarometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Electronic barometer" ;
    rdfs:subClassOf tern-ssn:Barometer .

tern-ssn:FrequencyDomainReflectometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Frequency domain reflectometer" ;
    rdfs:comment "A frequency domain reflectometry probe can measure the capitance from the change in frequency of a reflected radio wave or resonance frequency. An oscillator propagates an electromagnetic signal through a metal tine or other wave guide, and the difference in the output wave and the return wave frequency determines the soil moisture." ;
    rdfs:seeAlso <https://soilsensor.com/articles/capacitance-frequency-frequency-domain-reflectometry-fdr/> ;
    rdfs:subClassOf tern-ssn:SoilMoistureSensor ;
    skos:altLabel "FDR" .

tern-ssn:GravimetricHygrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Gravimetric hygrometer" ;
    rdfs:comment "A Gravimetric hygrometer measures the mass of an air sample compared to an equal volume of dry air." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Hygrometer#Gravimetric> ;
    rdfs:subClassOf tern-ssn:Hygrometer .

tern-ssn:HairHygrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Hair hygrometer" ;
    rdfs:comment """A hygrometer that measures relative humidity by means of the variation in length of a strand of human hair.

The length variation of a properly treated hair is 2%–2.5% when the humidity changes from 0%–100%. The hair hygrometer is considered to be a satisfactory instrument in situations where extreme and very low humidities are seldom or never found. The rate of response is very dependent on air temperature; the lag time increases with decreasing temperature. For air temperatures between 0° and 30°C and relative humidities between 20% and 80%, a good hair hygrometer should indicate 90% of a sudden change in humidity within about three minutes.""" ;
    rdfs:seeAlso <http://glossary.ametsoc.org/wiki/Hair_hygrometer> ;
    rdfs:subClassOf tern-ssn:Hygrometer .

tern-ssn:HeatFluxSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Heat flux sensor" ;
    rdfs:comment "A heat flux sensor is a transducer that generates an electrical signal proportional to the total heat rate applied to the surface of the sensor. The measured heat rate is divided by the surface area of the sensor to determine the heat flux." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Heat_flux_sensor> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:HotWireAnemometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Hot wire anemometer" ;
    rdfs:comment "A hot-wire anemometer is a thermal transducer which has been widely used to measure instantaneous flow velocity." ;
    rdfs:seeAlso <https://www.sciencedirect.com/topics/engineering/hot-wire-anemometer> ;
    rdfs:subClassOf tern-ssn:Anemometer .

tern-ssn:Hygrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Hygrometer" ;
    rdfs:comment "A hygrometer is an instrument used to measure the amount of humidity and water vapour in the atmosphere, in soil, or in confined spaces." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Hygrometer> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:ImagingRadar a rdfs:Class,
        owl:Class ;
    rdfs:label "Imaging radar" ;
    rdfs:comment "An instrument that transmit signals at frequencies between 1 GHz to 10 GHz and measures the backscattered signals to generate microwave images of Earth’s surface." ;
    rdfs:seeAlso <http://www.eohandbook.com/eohb2014/sat_earth_obs_imaging_microwave.html> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:InSituLandBasedPlatform a rdfs:Class,
        owl:Class ;
    rdfs:label "In situ land-based platform" ;
    rdfs:subClassOf sosa:Platform .

tern-ssn:InfraredGasAnalyzer a rdfs:Class,
        owl:Class ;
    rdfs:label "Infrared gas analyzer" ;
    rdfs:comment "An infrared gas analyzer measures trace gases by determining the absorption of an emitted infrared light source through a certain air sample." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Infrared_gas_analyzer> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:Lidar a rdfs:Class,
        owl:Class ;
    rdfs:label "Lidar" ;
    rdfs:comment "An instrument that emits laser and measures the radiation returned from the illuminated surface." ;
    rdfs:seeAlso <http://www.eohandbook.com/eohb2014/sat_earth_obs_lidars.html> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:LithiumChlorideHeatedCondensationHygrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Lithium chloride heated condensation hygrometer" ;
    rdfs:comment "The equilibrium vapour pressure at the surface of a saturated lithium chloride solution is exceptionally low. As a consequence, a solution of lithium chloride is extremely hygroscopic under typical conditions of surface atmospheric humidity; if the ambient vapour pressure exceeds the equilibrium vapour pressure of the solution, water vapour will condense over it (for example, at 0 °C water vapour condenses over a plane surface of a saturated solution of lithium chloride to only 15 %rh)." ;
    rdfs:seeAlso <https://www.wmo.int/pages/prog/www/IMOP/publications/CIMO-Guide/Prelim_2018_ed/8_I_4_en_MR_clean.pdf> ;
    rdfs:subClassOf tern-ssn:Hygrometer .

tern-ssn:MercuryBarometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Mercury barometer" ;
    rdfs:comment "A mercury barometer has a vertical glass tube closed at the top sitting in an open mercury-filled basin at the bottom. Mercury in the tube adjusts until the weight of the mercury column balances the atmospheric force exerted on the reservoir. High atmospheric pressure places more force on the reservoir, forcing mercury higher in the column. Low pressure allows the mercury to drop to a lower level in the column by lowering the force placed on the reservoir. Since higher temperature levels around the instrument will reduce the density of the mercury, the scale for reading the height of the mercury is adjusted to compensate for this effect. The tube has to be at least as long as the amount dipping in the mercury + head space + the maximum length of the column." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Barometer#Mercury_barometers> ;
    rdfs:subClassOf tern-ssn:Barometer .

tern-ssn:NetRadiometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Net radiometer" ;
    rdfs:comment "A net radiometer is a type of actinometer used to measure net radiation (NR) at the Earth's surface for meteorological applications." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Net_radiometer> ;
    rdfs:subClassOf tern-ssn:Actinometer .

tern-ssn:OpticalRainGauge a rdfs:Class,
        owl:Class ;
    rdfs:label "Optical rain gauge" ;
    rdfs:comment "This type of gauge has a row of collection funnels. In an enclosed space below each is a laser diode and a photo transistor detector. When enough water is collected to make a single drop, it drops from the bottom, falling into the laser beam path. The sensor is set at right angles to the laser so that enough light is scattered to be detected as a sudden flash of lights. The flashes from these photodetectors are then read and transmitted or recorded." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Rain_gauge#Optical_rain_gauge> ;
    rdfs:subClassOf tern-ssn:RainGauge .

tern-ssn:Photograph a rdfs:Class,
        owl:Class ;
    rdfs:label "Photograph" ;
    rdfs:comment "A sub-sample of the feature of interest as a photograph." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tern-ssn:PhotopointCollection ;
            owl:onProperty sosa:isSampleOf ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty sosa:isSampleOf ],
        tern-ssn:CollectedSample .

tern-ssn:PhotopointCollection a rdfs:Class,
        owl:Class ;
    rdfs:label "Photopoint collection" ;
    rdfs:comment "A sub-sample of the feature of interest as a series of photopoints." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom tern-ssn:Photograph ;
            owl:onProperty sosa:hasSample ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty sosa:hasSample ],
        tern-ssn:CollectedSample .

tern-ssn:PlantGenetics a rdfs:Class,
        owl:Class ;
    rdfs:label "Plant genetics" ;
    rdfs:comment "A plant genetics sample." ;
    rdfs:subClassOf tern-ssn:CollectedSample .

tern-ssn:PlantSpecimen a rdfs:Class,
        owl:Class ;
    rdfs:label "Plant specimen" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom dwc:Identification ;
            owl:onProperty dwc:nameAccordingTo ],
        tern-ssn:CollectedSample .

tern-ssn:PoleTower a rdfs:Class,
        owl:Class ;
    rdfs:label "Pole tower" ;
    rdfs:subClassOf tern-ssn:EddyCovarianceFluxTower .

tern-ssn:Psychrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Psychrometer" ;
    rdfs:comment """A psychrometer measures the relative humidity in the atmosphere through the use of two thermometers: 

1. a dry bulb thermometer, is used to measure the temperature by being exposed to the air. 
2. a wet bulb thermometer, measures temperature by having the bulb dipped in a liquid.""" ;
    rdfs:seeAlso <https://kestrelmeters.com/blogs/news/6638286-what-is-a-psychrometer> ;
    rdfs:subClassOf tern-ssn:Hygrometer .

tern-ssn:Pyranometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Pyranometer" ;
    rdfs:comment "A pyranometer is a type of actinometer used for measuring solar irradiance on a planar surface and it is designed to measure the solar radiation flux density (W/m²) from the hemisphere above within a wavelength range 0.3 μm to 3 μm." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Pyranometer> ;
    rdfs:subClassOf tern-ssn:Actinometer .

tern-ssn:Pyrgeometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Pyrgeometer" ;
    rdfs:comment "A pyrgeometer is a device that measures near-surface infra-red radiation spectrum in the wavelength spectrum approximately from 4.5 μm to 100 μm." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Pyrgeometer> ;
    rdfs:subClassOf tern-ssn:RadiationSensor .

tern-ssn:Pyrheliometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Pyrheliometer" ;
    rdfs:comment "A pyrheliometer is an instrument for measurement of direct beam solar irradiance." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Pyrheliometer> ;
    rdfs:subClassOf tern-ssn:RadiationSensor .

tern-ssn:Pyrradiometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Pyrradiometer" ;
    rdfs:comment "An instrument for measuring the total radiation, solar and terrestrial, falling from the solid angle 2 pi on a plane surface." ;
    rdfs:seeAlso <https://www.energy.eu/dictionary/data/1446.html> ;
    rdfs:subClassOf tern-ssn:Radiometer .

tern-ssn:QuantumSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Quantum sensor" ;
    rdfs:comment "A quantum sensor is a quantum device that responds to a stimulus. Usually this refers to a sensor which has quantized energy levels, uses quantum coherence to measure physical quantity, or uses entanglement to improve measurements beyond what can be done with classical sensors." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Quantum_sensor> ;
    rdfs:subClassOf tern-ssn:RadiationSensor .

tern-ssn:RadiationSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Radiation sensor" ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:Radiometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Radiometer" ;
    rdfs:comment "A radiometer or roentgenometer is a device for measuring the radiant flux (power) of electromagnetic radiation. Generally, a radiometer is an infrared radiation detector or an ultraviolet detector." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Radiometer> ;
    rdfs:subClassOf tern-ssn:RadiationSensor .

tern-ssn:RainGauge a rdfs:Class,
        owl:Class ;
    rdfs:label "Rain gauge" ;
    rdfs:comment "A rain gauge (also known as an udometer, pluviometer, or an ombrometer) is an instrument used by meteorologists and hydrologists to gather and measure the amount of liquid precipitation over an area in a predefined period of time." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Rain_gauge> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:ResistanceThermometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Resistance thermometer" ;
    rdfs:comment "Resistance thermometers, also called resistance temperature detectors (RTDs), are sensors used to measure temperature." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Resistance_thermometer> ;
    rdfs:subClassOf tern-ssn:TemperatureSensor ;
    skos:altLabel "Electrical resistance thermometer" .

tern-ssn:ResistiveHygrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Resistive hygrometer" ;
    rdfs:comment "In resistive hygrometers, the change in electrical resistance of a material due to humidity is measured." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Hygrometer#Resistive> ;
    rdfs:subClassOf tern-ssn:Hygrometer ;
    skos:altLabel "Electrical resistive hygrometer" .

tern-ssn:SapFlowSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Sap flow sensor" ;
    rdfs:comment "A sensor that measures sap flow. Sap flow is the movement of fluid in the roots, stems and branches of plants. Sap flow is typically measured in the xylem of plants. More specifically, sap flow is measured in the sapwood portion of xylem, or the conducting wood, which differs to the other portion of xylem known as heartwood." ;
    rdfs:seeAlso <https://www.edaphic.com.au/products/sap-flow-sensors/> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:SoilMetagenomics a rdfs:Class,
        owl:Class ;
    rdfs:label "Soil metagenomics" ;
    rdfs:comment "A soil metagenomics sample." ;
    rdfs:subClassOf tern-ssn:CollectedSample .

tern-ssn:SoilMoisturePlatform a rdfs:Class,
        owl:Class ;
    rdfs:label "Soil moisture platform" ;
    rdfs:subClassOf tern-ssn:InSituLandBasedPlatform .

tern-ssn:SoilMoistureSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Soil moisture sensor" ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:SoilSpecimen a rdfs:Class,
        owl:Class ;
    rdfs:label "Soil specimen" ;
    rdfs:comment "A soil sample." ;
    rdfs:subClassOf tern-ssn:CollectedSample .

tern-ssn:SonicAnemometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Sonic anemometer" ;
    rdfs:subClassOf tern-ssn:Anemometer .

tern-ssn:Spectrometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Spectrometer" ;
    rdfs:comment "A spectroradiometer is a light measurement tool that is able to measure both the wavelength and amplitude of the light emitted from a light source." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Spectroradiometer> ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:SuperSite a rdfs:Class,
        owl:Class ;
    rdfs:label "SuperSite" ;
    rdfs:subClassOf tern-ssn:EcologySite .

tern-ssn:SurveillanceSite a rdfs:Class,
        owl:Class ;
    rdfs:label "Surveillance site" ;
    rdfs:subClassOf tern-ssn:EcologySite .

tern-ssn:TemperatureSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Temperature sensor" ;
    rdfs:subClassOf sosa:Sensor .

tern-ssn:Thermistor a rdfs:Class,
        owl:Class ;
    rdfs:label "Thermistor" ;
    rdfs:comment "A thermistor is a type of resistor whose resistance is dependent on temperature, more so than in standard resistors. The word is a combination of thermal and resistor. Thermistors are widely used as inrush current limiters, temperature sensors (negative temperature coefficient or NTC type typically), self-resetting overcurrent protectors, and self-regulating heating elements (positive temperature coefficient or PTC type typically)." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Thermistor> ;
    rdfs:subClassOf tern-ssn:TemperatureSensor .

tern-ssn:Thermocouple a rdfs:Class,
        owl:Class ;
    rdfs:label "Thermocouple" ;
    rdfs:comment "A Thermocouple is a sensor used to measure temperature. Thermocouples consist of two wire legs made from different metals. The wires legs are welded together at one end, creating a junction. This junction is where the temperature is measured. When the junction experiences a change in temperature, a voltage is created. The voltage can then be interpreted using thermocouple reference tables to calculate the temperature." ;
    rdfs:seeAlso <https://www.thermocoupleinfo.com/index.htm> ;
    rdfs:subClassOf tern-ssn:TemperatureSensor .

tern-ssn:TimeDomainReflectometer a rdfs:Class,
        owl:Class ;
    rdfs:label "Time domain reflectometer" ;
    rdfs:comment """Sensors that use the Time Domain Reflectometry (TDR) are somewhat similar way to FDR probes, but the mechanics behind the measurement system are different. TDR sensors use parallel rods, acting as transmission lines. A voltage is applied to the rods and reflected back to the sensor for analysis. The speed or velocity of the voltage pulse along the rod is related to the apparent permittivity of the substrate (Blonquist 2005-A). In relatively wet soil the velocity of the pulse is slower than in drier soil.

Examples of this sensor include the Campbell Scientific CS650 and the IMKO Trime-Piko 32.""" ;
    rdfs:seeAlso <https://soilsensor.com/articles/time-domain-reflectometry-tdr/> ;
    rdfs:subClassOf tern-ssn:SoilMoistureSensor ;
    skos:altLabel "TDR" .

tern-ssn:TippingBucketRainGauge a rdfs:Class,
        owl:Class ;
    rdfs:label "Tipping bucket rain gauge" ;
    rdfs:comment "The tipping bucket rain gauge consists of a funnel that collects and channels the precipitation into a small seesaw-like container." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Rain_gauge#Tipping_bucket_rain_gauge> ;
    rdfs:subClassOf tern-ssn:RainGauge .

tern-ssn:TriangleTower a rdfs:Class,
        owl:Class ;
    rdfs:label "Triangle tower" ;
    rdfs:subClassOf tern-ssn:EddyCovarianceFluxTower .

tern-ssn:TripodTower a rdfs:Class,
        owl:Class ;
    rdfs:label "Tripod tower" ;
    rdfs:subClassOf tern-ssn:EddyCovarianceFluxTower .

tern-ssn:UAV a rdfs:Class,
        owl:Class ;
    rdfs:label "UAV" ;
    rdfs:subClassOf tern-ssn:Aircraft .

tern-ssn:WalkUpTower a rdfs:Class,
        owl:Class ;
    rdfs:label "Walk-up Tower" ;
    rdfs:subClassOf tern-ssn:EddyCovarianceFluxTower .

tern-ssn:WeighingPrecipitationGauge a rdfs:Class,
        owl:Class ;
    rdfs:label "Weighing precipitation gauge" ;
    rdfs:comment "A weighing-type precipitation gauge consists of a storage bin, which is weighed to record the mass." ;
    rdfs:seeAlso <https://en.wikipedia.org/wiki/Rain_gauge#Weighing_precipitation_gauge> ;
    rdfs:subClassOf tern-ssn:RainGauge ;
    skos:altLabel "Weighing rain gauge" .

tern-ssn:WindSensor a rdfs:Class,
        owl:Class ;
    rdfs:label "Wind sensor" ;
    rdfs:subClassOf sosa:Sensor .

