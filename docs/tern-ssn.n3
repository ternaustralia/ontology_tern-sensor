ssn-system:SystemProperty a owl:Class ;
    rdfs:label "System Property"@en ;
    rdfs:comment "An identifiable and observable characteristic that represents the System's ability to operate its primary purpose: a Sensor to make Observations, an Actuator to make Actuations, or a Sampler to make Samplings."@en ;
    rdfs:isDefinedBy ssn-system: ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom ssn-system:SystemCapability ;
            owl:onProperty [ owl:inverseOf ssn-system:hasSystemProperty ] ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty [ owl:inverseOf ssn-system:hasSystemProperty ] ],
        ssn:Property ;
    skos:definition "An identifiable and observable characteristic that represents the System's ability to operate its primary purpose: a Sensor to make Observations, an Actuator to make Actuations, or a Sampler to make Samplings."@en .

dwc:Identification a owl:Class ;
    rdfs:label "Identification" ;
    rdfs:comment "A taxonomic determination (e.g., the assignment to a taxon)." ;
    rdfs:isDefinedBy dwc: ;
    skos:example "A subspecies determination of an organism." .

dwc:Taxon a owl:Class ;
    rdfs:label "Taxon" ;
    rdfs:comment "A group of organisms (sensu http://purl.obolibrary.org/obo/OBI_0100026) considered by taxonomists to form a homogeneous unit." ;
    rdfs:isDefinedBy dwc: ;
    skos:example "The genus Truncorotaloides as published by Brönnimann et al. in 1953 in the Journal of Paleontology Vol. 27(6) p. 817-820." .

dwc:dateIdentified a rdf:Property ;
    rdfs:label "dateIdentified" ;
    rdfs:comment "The date on which the subject was identified as representing the Taxon." ;
    rdfs:isDefinedBy dwc: ;
    skos:example "1809-02-12 (some time during 12 February 1809).",
        "1900/1909 (some time during the interval between the beginning of the year 1900 and the end of the year 1909).",
        "1906-06 (some time in June 1906). ",
        "1963-03-08T14:07-0600 (8 Mar 1963 at 2:07pm in the time zone six hours earlier than UTC).",
        "1971 (some time in the year 1971).",
        "2007-03-01T13:00:00Z/2008-05-11T15:30:00Z (some time during the interval between 1 March 2007 1pm UTC and 11 May 2008 3:30pm UTC).",
        "2007-11-13/15 (some time in the interval between 13 November 2007 and 15 November 2007).",
        "2009-02-20T08:40Z (20 February 2009 8:40am UTC).",
        "2018-08-29T15:19 (3:19pm local time on 29 August 2018)." ;
    skos:note "Recommended best practice is to use a date that conforms to ISO 8601:2004(E)." .

dwc:materialSampleID a rdf:Property ;
    rdfs:label "material sample ID" ;
    rdfs:comment "An identifier for the MaterialSample (as opposed to a particular digital record of the material sample). In the absence of a persistent global unique identifier, construct one from a combination of identifiers in the record that will most closely make the materialSampleID globally unique." ;
    rdfs:isDefinedBy dwc: ;
    skos:example "06809dc5-f143-459a-be1a-6f03e63fc083" .

dwc:scientificNameID a rdf:Property ;
    rdfs:label "scientific name ID" ;
    rdfs:comment "An identifier for the nomenclatural (not taxonomic) details of a scientific name." ;
    rdfs:isDefinedBy dwc: ;
    skos:example "urn:lsid:ipni.org:names:37829-1:1.3" .

dwc:taxonConceptID a rdf:Property ;
    rdfs:label "taxon concept ID" ;
    rdfs:comment "An identifier for the taxonomic concept to which the record refers - not for the nomenclatural details of a taxon." ;
    rdfs:isDefinedBy dwc: ;
    skos:example "8fa58e08-08de-4ac1-b69c-1235340b7001" .

schema:manufacturer rdfs:range tern-org:Manufacturer .

<http://www.w3.org/ns/prov#Entity> rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom schema:Person ;
            owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty schema:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ] .

sosa:hosts a owl:ObjectProperty ;
    rdfs:label "hosts"@en ;
    schema:domainIncludes sosa:Platform ;
    schema:rangeIncludes sosa:Actuator,
        sosa:Platform,
        sosa:Sampler,
        sosa:Sensor ;
    rdfs:comment "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en ;
    rdfs:isDefinedBy sosa: ;
    owl:inverseOf sosa:isHostedBy ;
    owl:propertyChainAxiom ( ssn:inDeployment ssn:deployedSystem ) ;
    skos:definition "Relation between a Platform and a Sensor, Actuator, Sampler, or Platform, hosted or mounted on it."@en .

ssn:deployedOnPlatform a owl:ObjectProperty ;
    rdfs:label "deployed on platform"@en ;
    rdfs:comment "Relation between a Deployment and the Platform on which the Systems are deployed."@en ;
    rdfs:isDefinedBy ssn: ;
    owl:inverseOf ssn:inDeployment ;
    skos:definition "Relation between a Deployment and the Platform on which the Systems are deployed."@en .

tern-ssn:DeploymentInfo a owl:Class ;
    rdfs:label "Deployment info" ;
    rdfs:comment "Deployment information of a system." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty dct:subject ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:Deployment ;
            owl:onProperty [ owl:inverseOf tern-ssn:deploymentInfo ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:System ;
            owl:onProperty tern-ssn:onSystem ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dct:subject ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty tern-ssn:onSystem ],
        owl:Thing .

tern-ssn:NationalTaxon a owl:Class ;
    rdfs:label "National taxon" ;
    rdfs:comment "A taxon identified by the national herbarium." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dwc:scientificName ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:comment ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dwc:scientificName ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dwc:scientificNameID ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty dwc:taxonConceptID ],
        [ a owl:Restriction ;
            owl:cardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty rdfs:label ],
        [ a owl:Restriction ;
            owl:onProperty dwc:scientificNameID ;
            owl:someValuesFrom xsd:anyURI ],
        [ a owl:Restriction ;
            owl:onProperty dwc:taxonConceptID ;
            owl:someValuesFrom xsd:anyURI ],
        dwc:Taxon .

tern-ssn:actualResult a rdf:Property ;
    rdfs:label "actual result" ;
    rdfs:subPropertyOf tern-ssn:sensorCalibrationValue .

tern-ssn:fluxnetID a rdf:Property ;
    rdfs:label "fluxnet ID" ;
    rdfs:comment "The fluxnet ID is the SITE_ID of the flux site found at https://fluxnet.fluxdata.org/sites/site-list-and-pages/" .

tern-ssn:nationalIdentification a rdf:Property ;
    rdfs:label "national identification" ;
    rdfs:comment "Link an Identification or Taxon to a National Taxon." ;
    rdfs:subPropertyOf dwc:identificationID .

tern-ssn:onSystem a owl:ObjectProperty ;
    rdfs:label "on system" ;
    rdfs:comment "Link deployment information to a system." ;
    rdfs:domain tern-ssn:DeploymentInfo .

tern-ssn:referenceResult a rdf:Property ;
    rdfs:label "reference result" ;
    rdfs:comment "The standard reference value used to calibrate the sensors. " ;
    rdfs:subPropertyOf tern-ssn:sensorCalibrationValue .

tern-ssn:sensorConfigurationValue a rdf:Property ;
    rdfs:label "sensor configuration value" .

tern-ssn:stateIdentification a rdf:Property ;
    rdfs:label "state identification" ;
    rdfs:comment "Link an Identification or a Taxon to a State Taxon." ;
    rdfs:subPropertyOf dwc:identificationID .

<https://w3id.org/tern/resources/a083902d-d821-41be-b663-1d7cb33eea66> schema:alternateName "TERN" ;
    schema:name "Terrestrial Ecosystem Research Network" ;
    schema:url <https://www.tern.org.au/> .

ssn-system:SystemCapability a owl:Class ;
    rdfs:label "System Capability"@en ;
    rdfs:comment """Describes normal measurement, actuation, sampling properties such as accuracy, range, precision, etc. of a System under some specified Conditions such as a temperature range.

    The capabilities specified here are those that affect the primary purpose of the System, while those in OperatingRange represent the system's normal operating environment, including conditions that don't affect the observations or the actuations."""@en ;
    rdfs:isDefinedBy ssn-system: ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom ssn:Property ;
            owl:onProperty ssn:forProperty ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:System ;
            owl:onProperty [ owl:inverseOf ssn-system:hasSystemCapability ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn-system:Condition ;
            owl:onProperty ssn-system:inCondition ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn-system:SystemProperty ;
            owl:onProperty ssn-system:hasSystemProperty ],
        [ a owl:Restriction ;
            owl:minCardinality "1"^^xsd:nonNegativeInteger ;
            owl:onProperty ssn-system:inCondition ],
        ssn:Property ;
    skos:definition """Describes normal measurement, actuation, sampling properties such as accuracy, range, precision, etc. of a System under some specified Conditions such as a temperature range.

    The capabilities specified here are those that affect the primary purpose of the System, while those in OperatingRange represent the system's normal operating environment, including conditions that don't affect the observations or the actuations."""@en .

ssn-system:hasSystemCapability a owl:ObjectProperty ;
    rdfs:label "has system capability"@en ;
    rdfs:comment "Relation from a System to a SystemCapability describing the capabilities of the System under certain Conditions."@en ;
    rdfs:isDefinedBy ssn-system: ;
    rdfs:subPropertyOf ssn:hasProperty ;
    skos:definition "Relation from a System to a SystemCapability describing the capabilities of the System under certain Conditions."@en .

dwc:identificationID a rdf:Property ;
    rdfs:label "identification ID" ;
    rdfs:comment "An identifier for the Identification (the body of information associated with the assignment of a scientific name). May be a global unique identifier or an identifier specific to the data set." ;
    rdfs:isDefinedBy dwc: ;
    skos:example "9992" .

sosa:isHostedBy a owl:ObjectProperty ;
    rdfs:label "is hosted by"@en ;
    schema:domainIncludes sosa:Actuator,
        sosa:Platform,
        sosa:Sampler,
        sosa:Sensor ;
    schema:rangeIncludes sosa:Platform ;
    rdfs:comment "Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by."@en ;
    rdfs:isDefinedBy sosa: ;
    owl:inverseOf sosa:hosts ;
    skos:definition "Relation between a Sensor, Actuator, Sampler, or Platform, and the Platform that it is mounted on or hosted by."@en .

ssn:deployedSystem a owl:ObjectProperty ;
    rdfs:label "deployed system"@en ;
    rdfs:comment "Relation between a Deployment and a deployed System."@en ;
    rdfs:isDefinedBy ssn: ;
    owl:inverseOf ssn:hasDeployment ;
    skos:definition "Relation between a Deployment and a deployed System."@en .

ssn:hasDeployment a owl:ObjectProperty ;
    rdfs:label "has deployment"@en ;
    rdfs:comment "Relation between a System and a Deployment, recording that the System is deployed in that Deployment."@en ;
    rdfs:isDefinedBy ssn: ;
    owl:inverseOf ssn:deployedSystem ;
    skos:definition "Relation between a System and a Deployment, recording that the System is deployed in that Deployment."@en .

ssn:inDeployment a owl:ObjectProperty ;
    rdfs:label "in deployment"@en ;
    rdfs:comment "Relation between a Platform and a Deployment, meaning that the deployedSystems of the Deployment are hosted on the Platform."@en ;
    rdfs:isDefinedBy ssn: ;
    owl:inverseOf ssn:deployedOnPlatform ;
    skos:definition "Relation between a Platform and a Deployment, meaning that the deployedSystems of the Deployment are hosted on the Platform."@en ;
    skos:example "For example, a relation between a buoy and a deployment of several Sensors."@en .

ssn-system:hasSystemProperty a owl:ObjectProperty ;
    rdfs:label "has system property"@en ;
    rdfs:comment "Relation from an SystemCapability of a System to a SystemProperty describing the capabilities of the System."@en ;
    rdfs:isDefinedBy ssn-system: ;
    rdfs:subPropertyOf ssn:hasProperty ;
    skos:definition "Relation from an SystemCapability of a System to a SystemProperty describing the capabilities of the System."@en .

dwc:scientificName a rdf:Property ;
    rdfs:label "scientific name" ;
    rdfs:comment "The full scientific name, with authorship and date information if known. When forming part of an Identification, this should be the name in lowest level taxonomic rank that can be determined. This term should not contain identification qualifications, which should instead be supplied in the IdentificationQualifier term." ;
    rdfs:isDefinedBy dwc: .

tern-ssn:sensorCalibrationValue a rdf:Property ;
    rdfs:label "sensor calibration value" .

ssn:Deployment a owl:Class ;
    rdfs:label "Deployment"@en ;
    rdfs:comment "Describes the Deployment of one or more Systems for a particular purpose. Deployment may be done on a Platform."@en ;
    rdfs:isDefinedBy ssn: ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom schema:Place ;
            owl:onProperty schema:location ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:QuantitativeValue ;
            owl:onProperty tern-ssn:deploymentDepth ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:QuantitativeValue ;
            owl:onProperty tern-ssn:deploymentHeight ],
        [ a owl:Restriction ;
            owl:allValuesFrom schema:QuantitativeValue ;
            owl:onProperty tern-ssn:deploymentOrientation ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <http://www.w3.org/ns/prov#generatedAtTime> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <http://www.w3.org/ns/prov#invalidatedAtTime> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty schema:description ],
        [ a owl:Restriction ;
            owl:allValuesFrom skos:Concept ;
            owl:onProperty tern-ssn:deploymentType ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasGeneratedBy> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://www.w3.org/ns/prov#Activity> ;
            owl:onProperty <http://www.w3.org/ns/prov#wasInvalidatedBy> ],
        [ a owl:Restriction ;
            owl:allValuesFrom sosa:Platform ;
            owl:onProperty ssn:deployedOnPlatform ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:Property ;
            owl:onProperty ssn:forProperty ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:System ;
            owl:onProperty ssn:deployedSystem ],
        [ a owl:Restriction ;
            owl:onProperty <http://www.w3.org/ns/prov#wasAttributedTo> ;
            owl:someValuesFrom schema:Person ] ;
    skos:definition "Describes the Deployment of one or more Systems for a particular purpose. Deployment may be done on a Platform."@en ;
    skos:example "For example, a temperature Sensor deployed on a wall, or a whole network of Sensors deployed for an Observation campaign."@en .

<http://www.w3.org/ns/prov#Activity> rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <http://www.w3.org/ns/prov#endedAtTime> ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:dateTime ;
            owl:onProperty <http://www.w3.org/ns/prov#startedAtTime> ],
        [ a owl:Restriction ;
            owl:onProperty <http://www.w3.org/ns/prov#wasAssociatedWith> ;
            owl:someValuesFrom schema:Person ] .

ssn:System a owl:Class ;
    rdfs:label "System"@en ;
    rdfs:comment "System is a unit of abstraction for pieces of infrastructure that implement Procedures. A System may have components, its subsystems, which are other systems."@en ;
    rdfs:isDefinedBy ssn: ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom sosa:Platform ;
            owl:onProperty sosa:isHostedBy ],
        [ a owl:Restriction ;
            owl:allValuesFrom sosa:Procedure ;
            owl:onProperty ssn:implements ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:Deployment ;
            owl:onProperty ssn:hasDeployment ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:System ;
            owl:onProperty ssn:hasSubSystem ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn:System ;
            owl:onProperty [ owl:inverseOf ssn:hasSubSystem ] ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn-system:OperatingRange ;
            owl:onProperty ssn-system:hasOperatingRange ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn-system:SurvivalRange ;
            owl:onProperty ssn-system:hasSurvivalRange ],
        [ a owl:Restriction ;
            owl:allValuesFrom ssn-system:SystemCapability ;
            owl:onProperty ssn-system:hasSystemCapability ] ;
    skos:definition "System is a unit of abstraction for pieces of infrastructure that implement Procedures. A System may have components, its subsystems, which are other systems."@en .

