            owl:allValuesFrom xsd:string ;
            owl:onProperty dwc:identificationID ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dwc:identificationRemarks ],
        [ a owl:Restriction ;
            owl:allValuesFrom xsd:string ;
            owl:onProperty dwc:identifiedBy ],
        [ a owl:Restriction ;
